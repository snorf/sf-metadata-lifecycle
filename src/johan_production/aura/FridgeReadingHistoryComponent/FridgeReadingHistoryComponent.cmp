<aura:component description="FridgeReadingHistoryComponent"  controller="FridgeReadingHistoryController">
    <ltng:require scripts="{!$Resource.chartjs_270}" />
    <aura:handler name="init" value="{!this}" action="{!c.doinit}"/>
    <div class="container">
        <div class="chart-demo">
            <h2 class="title">Fridge</h2>
            <div>
                <canvas id="temperature" style="display: block; height: 560px; width: 1120px;" width="640" height="480" class="chartjs-render-monitor"></canvas>
            </div>
            <div>
                <canvas id="humidity" style="display: block; height: 560px; width: 1120px;" width="640" height="480" class="chartjs-render-monitor"></canvas>
            </div>
            <div>
                <canvas id="door" style="display: block; height: 560px; width: 1120px;" width="640" height="480" class="chartjs-render-monitor"></canvas>
            </div>
        </div>
    </div>
</aura:component>